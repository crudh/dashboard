{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "ecmaFeatures": {
    "modules": true
  },
  "rules": {
    "comma-dangle": [2, "never"],
    "curly": 0,
    "max-len": [2, 120, 4],
    "quotes": [2, "double"],
    "react/no-multi-comp": 0,
    "react/jsx-closing-bracket-location": 0
  }
}
